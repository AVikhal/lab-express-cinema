<div>
    <a href="/">Go back</a>
    <h1>Ironhack Cinema</h1>
    <br>
    <h3>Click on the movie to check the showtime!</h3>
</div>

<div>
    <ul>
        {{#each this.moviesList}}
        <div>

            <img src="{{this.image}}" alt="movie">
            <h3>{{this.title}}</h3>
            <a href="/movie/{{this.id}}">See more</a>
        
        </div>
        
        {{/each}}
    </ul>
</div>